#include <stdio.h>

int main() {
    int total;
    int coin[3]={1,2,5};
    scanf("%d",&total);
    while(total>0)
    {
        if(total>coin[2])
        {
            total-=coin[2];
            printf("%d,",coin[2]);
        }
         else if(total>coin[1])
        {
            total-=coin[1];
            printf("%d,",coin[1]);
        }
        else
        {
            total-=coin[0];
            printf("%d",coin[0]);
        }
    }

    return 0;
}
